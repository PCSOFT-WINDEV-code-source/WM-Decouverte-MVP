  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre()
      
      gclTableContact est un CPresentationTableContact(gclTousLesContacts) <présentation>
      gpclLeNouveauContact est un CPresentationFicheContact dynamique
   -
     code : |1+
      // On affiche une info au premier "go"
      SI EnModeTestAutomatique() = Faux ALORS	
      	info(<§$0000§>)
      FIN
     type : 34
   -
     code : |1+
      TableAffiche(ZR_Contact,taCourantBandeau)
     type : 234
   -
     code : |1+
      si MaFenêtreFille..ValeurRenvoyée = Vrai ALORS
      	
      	// On a alloué un membre "nouveau" ? 
      	si gpclLeNouveauContact = Null ALORS
      		// On doit modifier le contact dans la liste des contacts
      		gclTableContact.ModifierContact()
      		
      	sinon		
      		// Il suffit de l'ajouter
      		gclTableContact.AjouterContact(gpclLeNouveauContact)
      		
      		// Libère l'élément
      		gpclLeNouveauContact = null
      	FIN
      FIN
      
      SI ErreurDétectée ALORS Erreur()
