         SI SAI_Nom = "" ALORS
         	Info(<§$0001§>)
         	retour
         FIN
         
         // Affectation
         gpclFicheContact.p_Email = SAI_Email
         gpclFicheContact.p_Prénom = SAI_Prénom
         gpclFicheContact.p_Nom = SAI_Nom
         
         // Validation des modifications (ajout ou modification) par la couche de présentation
         gpclFicheContact.ValideModification()
         SI ErreurDétectée ALORS 
         	Erreur()
         	RETOUR
         FIN
         
         // Fermeture de la fenêtre en indiquant qu'une modification des données a été effectuée
         MaFenêtre..ValeurRenvoyée = Vrai
         Ferme()
         
         
