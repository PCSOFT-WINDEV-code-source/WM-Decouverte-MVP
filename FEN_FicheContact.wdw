   -
     code : |1-
      PROCEDURE MaFenêtre(gpclFicheContact est un CPresentationFicheContact dynamique <présentation>, gbNouveau est un booléen = faux)
      
      // Par défaut, la fenêtre n'est pas validée
      MaFenêtre..ValeurRenvoyée = Faux
   -
     code : |1+
      // Nouveau ou modification ? 
      si gbNouveau ALORS
      	BTN_Modifier..Libellé = "Ajouter"
      FIN
      
      // Mise à jour du titre de la fenêtre
      MoiMême..Titre=gpclFicheContact.p_sNomComplet
      
      // Affectation
      SAI_Email = gpclFicheContact.p_Email
      SAI_Prénom = gpclFicheContact.p_Prénom
      SAI_Nom = gpclFicheContact.p_Nom
      
      // Image de photo disponible ? 
      si gpclFicheContact.p_Photo <> "" ALORS	
      	IMG_Profil = gpclFicheContact.p_Photo
      FIN
      
      // Mise à jour des champs liés de la fenêtre
      SourceVersEcran()
